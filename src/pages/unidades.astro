---
import Layout from '../layouts/Layout.astro';
import Unidade from '../components/Unidades.astro';

const title = "Unidades | Autobuses en Venta";
const description = "Descubre nuestra variedad de unidades de autobuses nuevos y usados. Encuentra el modelo perfecto para tus necesidades de transporte público o privado.";
const keywords = "autobuses, unidades, venta de autobuses, vehículos de transporte, autobuses nuevos, autobuses usados";
---

<Layout title={title}>
  <Unidade description={description} keywords={keywords} />
</Layout>
